{
  "title": "Cache & API Performance",
  "tags": ["polymarket", "cache", "api", "performance"],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "10s",
  "templating": {
    "list": [
      {
        "name": "datasource",
        "type": "datasource",
        "query": "prometheus"
      },
      {
        "name": "operation",
        "type": "custom",
        "multi": true,
        "options": [
          {"text": "All", "value": ".*", "selected": true},
          {"text": "Get", "value": "get"},
          {"text": "Set", "value": "set"},
          {"text": "Delete", "value": "delete"}
        ],
        "current": {"text": "All", "value": ".*"}
      }
    ]
  },
  "panels": [
    {
      "id": 1,
      "title": "Cache Hit Rate",
      "type": "gauge",
      "gridPos": {"h": 8, "w": 8, "x": 0, "y": 0},
      "targets": [
        {
          "expr": "polymarket_cache_hit_rate{service=\"arb-bot\"} * 100",
          "refId": "A"
        }
      ],
      "options": {
        "showThresholdLabels": false,
        "showThresholdMarkers": true
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": 0, "color": "red"},
              {"value": 50, "color": "orange"},
              {"value": 80, "color": "green"}
            ]
          }
        }
      }
    },
    {
      "id": 2,
      "title": "Total Cache Hits (Last Hour)",
      "type": "stat",
      "gridPos": {"h": 4, "w": 8, "x": 8, "y": 0},
      "targets": [
        {
          "expr": "increase(polymarket_cache_hits_total{service=\"arb-bot\"}[1h])",
          "refId": "A"
        }
      ],
      "options": {
        "colorMode": "value",
        "graphMode": "area"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 0,
          "color": {"mode": "fixed", "fixedColor": "green"}
        }
      }
    },
    {
      "id": 3,
      "title": "Total Cache Misses (Last Hour)",
      "type": "stat",
      "gridPos": {"h": 4, "w": 8, "x": 16, "y": 0},
      "targets": [
        {
          "expr": "increase(polymarket_cache_misses_total{service=\"arb-bot\"}[1h])",
          "refId": "A"
        }
      ],
      "options": {
        "colorMode": "value",
        "graphMode": "area"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 0,
          "color": {"mode": "fixed", "fixedColor": "red"}
        }
      }
    },
    {
      "id": 4,
      "title": "Cache Operations Rate",
      "type": "stat",
      "gridPos": {"h": 4, "w": 8, "x": 8, "y": 4},
      "targets": [
        {
          "expr": "sum(rate(polymarket_cache_hits_total{service=\"arb-bot\"}[1m]) + rate(polymarket_cache_misses_total{service=\"arb-bot\"}[1m]))",
          "refId": "A"
        }
      ],
      "options": {
        "colorMode": "value",
        "graphMode": "area"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "decimals": 2,
          "color": {"mode": "palette-classic"}
        }
      }
    },
    {
      "id": 5,
      "title": "Cache Sets/sec",
      "type": "stat",
      "gridPos": {"h": 4, "w": 8, "x": 16, "y": 4},
      "targets": [
        {
          "expr": "rate(polymarket_cache_sets_total{service=\"arb-bot\"}[1m])",
          "refId": "A"
        }
      ],
      "options": {
        "colorMode": "value",
        "graphMode": "area"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "decimals": 2,
          "color": {"mode": "palette-classic"}
        }
      }
    },
    {
      "id": 6,
      "title": "Cache Hit Rate Timeline",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
      "targets": [
        {
          "expr": "(rate(polymarket_cache_hits_total{service=\"arb-bot\"}[1m]) / (rate(polymarket_cache_hits_total{service=\"arb-bot\"}[1m]) + rate(polymarket_cache_misses_total{service=\"arb-bot\"}[1m]))) * 100",
          "legendFormat": "Hit Rate %",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "decimals": 1,
          "color": {"mode": "palette-classic"},
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 20
          }
        }
      },
      "options": {
        "tooltip": {"mode": "single"},
        "legend": {"displayMode": "list", "placement": "bottom"}
      }
    },
    {
      "id": 7,
      "title": "Cache Hits vs Misses",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
      "targets": [
        {
          "expr": "rate(polymarket_cache_hits_total{service=\"arb-bot\"}[1m])",
          "legendFormat": "Hits/sec",
          "refId": "A"
        },
        {
          "expr": "rate(polymarket_cache_misses_total{service=\"arb-bot\"}[1m])",
          "legendFormat": "Misses/sec",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "decimals": 2,
          "color": {"mode": "palette-classic"},
          "custom": {
            "stacking": {"mode": "normal"},
            "fillOpacity": 30
          }
        }
      },
      "options": {
        "tooltip": {"mode": "multi"},
        "legend": {"displayMode": "table", "placement": "bottom", "calcs": ["mean", "max"]}
      }
    },
    {
      "id": 8,
      "title": "Cache Operation Latency (by Type)",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(polymarket_cache_operation_duration_seconds_bucket{service=\"arb-bot\",operation=~\"$operation\"}[1m])) * 1000000",
          "legendFormat": "{{operation}} (p95)",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "Âµs",
          "decimals": 2,
          "color": {"mode": "palette-classic"},
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10
          }
        }
      },
      "options": {
        "tooltip": {"mode": "multi"},
        "legend": {"displayMode": "table", "placement": "bottom", "calcs": ["mean", "max"]}
      }
    },
    {
      "id": 9,
      "title": "Market Metadata Fetch Rate",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
      "targets": [
        {
          "expr": "rate(polymarket_markets_metadata_fetched_total{service=\"arb-bot\"}[5m]) * 60",
          "legendFormat": "Fetches/min",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 2,
          "color": {"mode": "palette-classic"},
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 20
          }
        }
      },
      "options": {
        "tooltip": {"mode": "single"},
        "legend": {"displayMode": "list", "placement": "bottom"}
      }
    },
    {
      "id": 10,
      "title": "Metadata Fetch Duration (p50/p95)",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24},
      "targets": [
        {
          "expr": "histogram_quantile(0.50, rate(polymarket_markets_metadata_fetch_duration_seconds_bucket{service=\"arb-bot\"}[5m]))",
          "legendFormat": "p50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, rate(polymarket_markets_metadata_fetch_duration_seconds_bucket{service=\"arb-bot\"}[5m]))",
          "legendFormat": "p95",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "decimals": 3,
          "color": {"mode": "palette-classic"},
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10
          }
        }
      },
      "options": {
        "tooltip": {"mode": "multi"},
        "legend": {"displayMode": "table", "placement": "bottom", "calcs": ["mean", "max"]}
      }
    },
    {
      "id": 11,
      "title": "Metadata Cache Efficiency",
      "type": "piechart",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24},
      "targets": [
        {
          "expr": "sum(increase(polymarket_markets_metadata_cache_hits_total{service=\"arb-bot\"}[1h]))",
          "legendFormat": "Cache Hits",
          "refId": "A"
        },
        {
          "expr": "sum(increase(polymarket_markets_metadata_cache_misses_total{service=\"arb-bot\"}[1h]))",
          "legendFormat": "Cache Misses",
          "refId": "B"
        }
      ],
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "right",
          "values": ["value", "percent"]
        },
        "pieType": "donut",
        "tooltip": {
          "mode": "single"
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      }
    },
    {
      "id": 12,
      "title": "Discovery Poll Duration (p95)",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 24, "x": 0, "y": 32},
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(polymarket_discovery_poll_duration_seconds_bucket{service=\"arb-bot\"}[1m]))",
          "legendFormat": "p95 Poll Duration",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "decimals": 3,
          "color": {"mode": "palette-classic"},
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 20
          }
        }
      },
      "options": {
        "tooltip": {"mode": "single"},
        "legend": {"displayMode": "list", "placement": "bottom"}
      }
    }
  ],
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": ["10s", "30s", "1m", "5m", "15m"]
  }
}
