{
  "title": "Error Analysis & Debugging",
  "tags": ["polymarket", "errors", "debugging", "failures"],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "10s",
  "templating": {
    "list": [
      {
        "name": "datasource",
        "type": "datasource",
        "query": "prometheus"
      }
    ]
  },
  "panels": [
    {
      "id": 1,
      "title": "Execution Errors by Type",
      "type": "piechart",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
      "targets": [
        {
          "expr": "sum by(error_type) (increase(polymarket_execution_errors_by_type_total{service=\"arb-bot\"}[1h]))",
          "legendFormat": "{{error_type}}",
          "refId": "A"
        }
      ],
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "right",
          "values": ["value", "percent"]
        },
        "pieType": "donut",
        "tooltip": {
          "mode": "single"
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      }
    },
    {
      "id": 2,
      "title": "Error Ratio (Errors vs Success)",
      "type": "gauge",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
      "targets": [
        {
          "expr": "(sum(rate(polymarket_execution_errors_total{service=\"arb-bot\"}[5m])) / (sum(rate(polymarket_execution_trades_total{service=\"arb-bot\"}[5m])) + sum(rate(polymarket_execution_errors_total{service=\"arb-bot\"}[5m])))) * 100",
          "refId": "A"
        }
      ],
      "options": {
        "showThresholdLabels": false,
        "showThresholdMarkers": true
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": 0, "color": "green"},
              {"value": 1, "color": "yellow"},
              {"value": 5, "color": "red"}
            ]
          }
        }
      }
    },
    {
      "id": 3,
      "title": "Execution Error Rate",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
      "targets": [
        {
          "expr": "rate(polymarket_execution_errors_total{service=\"arb-bot\"}[1m]) * 60",
          "legendFormat": "Errors/min",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 2,
          "color": {"mode": "palette-classic"},
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 20
          }
        }
      },
      "options": {
        "tooltip": {"mode": "single"},
        "legend": {"displayMode": "list", "placement": "bottom"}
      }
    },
    {
      "id": 4,
      "title": "Execution Errors Timeline (by Type)",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
      "targets": [
        {
          "expr": "rate(polymarket_execution_errors_by_type_total{service=\"arb-bot\"}[1m]) * 60",
          "legendFormat": "{{error_type}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 2,
          "color": {"mode": "palette-classic"},
          "custom": {
            "stacking": {"mode": "normal"},
            "fillOpacity": 20
          }
        }
      },
      "options": {
        "tooltip": {"mode": "multi"},
        "legend": {"displayMode": "table", "placement": "bottom", "calcs": ["sum"]}
      }
    },
    {
      "id": 5,
      "title": "Opportunities Rejected (by Reason)",
      "type": "piechart",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
      "targets": [
        {
          "expr": "sum by(reason) (increase(polymarket_arb_opportunities_rejected_total{service=\"arb-bot\"}[1h]))",
          "legendFormat": "{{reason}}",
          "refId": "A"
        }
      ],
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "right",
          "values": ["value", "percent"]
        },
        "pieType": "donut",
        "tooltip": {
          "mode": "single"
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      }
    },
    {
      "id": 6,
      "title": "Rejection Rate Timeline (by Reason)",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
      "targets": [
        {
          "expr": "rate(polymarket_arb_opportunities_rejected_total{service=\"arb-bot\"}[5m]) * 60",
          "legendFormat": "{{reason}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 2,
          "color": {"mode": "palette-classic"},
          "custom": {
            "stacking": {"mode": "normal"},
            "fillOpacity": 30
          }
        }
      },
      "options": {
        "tooltip": {"mode": "multi"},
        "legend": {"displayMode": "table", "placement": "bottom", "calcs": ["sum"]}
      }
    },
    {
      "id": 7,
      "title": "Opportunities Skipped (by Reason)",
      "type": "piechart",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24},
      "targets": [
        {
          "expr": "sum by(reason) (increase(polymarket_execution_opportunities_skipped_total{service=\"arb-bot\"}[1h]))",
          "legendFormat": "{{reason}}",
          "refId": "A"
        }
      ],
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "right",
          "values": ["value", "percent"]
        },
        "pieType": "donut",
        "tooltip": {
          "mode": "single"
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      }
    },
    {
      "id": 8,
      "title": "Skipped Opportunity Rate (by Reason)",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24},
      "targets": [
        {
          "expr": "rate(polymarket_execution_opportunities_skipped_total{service=\"arb-bot\"}[5m]) * 60",
          "legendFormat": "{{reason}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 2,
          "color": {"mode": "palette-classic"},
          "custom": {
            "stacking": {"mode": "normal"},
            "fillOpacity": 30
          }
        }
      },
      "options": {
        "tooltip": {"mode": "multi"},
        "legend": {"displayMode": "table", "placement": "bottom", "calcs": ["sum"]}
      }
    },
    {
      "id": 9,
      "title": "WebSocket Errors",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 8, "x": 0, "y": 32},
      "targets": [
        {
          "expr": "rate(polymarket_ws_errors_total{service=\"arb-bot\"}[1m]) * 60",
          "legendFormat": "WS Errors/min",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 2,
          "color": {"mode": "fixed", "fixedColor": "red"},
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 20
          }
        }
      },
      "options": {
        "tooltip": {"mode": "single"},
        "legend": {"displayMode": "list", "placement": "bottom"}
      }
    },
    {
      "id": 10,
      "title": "Discovery Errors (API Failures)",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 8, "x": 8, "y": 32},
      "targets": [
        {
          "expr": "rate(polymarket_discovery_errors_total{service=\"arb-bot\"}[1m]) * 60",
          "legendFormat": "Discovery Errors/min",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 2,
          "color": {"mode": "fixed", "fixedColor": "orange"},
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 20
          }
        }
      },
      "options": {
        "tooltip": {"mode": "single"},
        "legend": {"displayMode": "list", "placement": "bottom"}
      }
    },
    {
      "id": 11,
      "title": "Wallet Update Errors (RPC Failures)",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 8, "x": 16, "y": 32},
      "targets": [
        {
          "expr": "rate(polymarket_wallet_update_errors_total{service=\"wallet-tracker\"}[1m]) * 60",
          "legendFormat": "Wallet Errors/min",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 2,
          "color": {"mode": "fixed", "fixedColor": "yellow"},
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 20
          }
        }
      },
      "options": {
        "tooltip": {"mode": "single"},
        "legend": {"displayMode": "list", "placement": "bottom"}
      }
    }
  ],
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": ["10s", "30s", "1m", "5m", "15m"]
  }
}
