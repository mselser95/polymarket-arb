{
  "title": "Polymarket Latency & Performance (HFT)",
  "tags": ["polymarket", "latency", "performance", "hft"],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "5s",
  "templating": {
    "list": [
      {
        "name": "datasource",
        "type": "datasource",
        "query": "prometheus"
      }
    ]
  },
  "panels": [
    {
      "id": 1,
      "title": "End-to-End Latency: Orderbook → Opportunity (p99)",
      "type": "stat",
      "gridPos": {"h": 4, "w": 8, "x": 0, "y": 0},
      "targets": [
        {
          "expr": "histogram_quantile(0.99, rate(polymarket_arb_e2e_latency_seconds_bucket[1m])) * 1000",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "decimals": 3,
          "thresholds": {
            "steps": [
              {"value": 0, "color": "green"},
              {"value": 1, "color": "yellow"},
              {"value": 10, "color": "red"}
            ]
          }
        }
      },
      "options": {
        "colorMode": "background",
        "graphMode": "area"
      }
    },
    {
      "id": 2,
      "title": "Detection Loop Latency (p99)",
      "type": "stat",
      "gridPos": {"h": 4, "w": 8, "x": 8, "y": 0},
      "targets": [
        {
          "expr": "histogram_quantile(0.99, rate(polymarket_arb_detection_duration_seconds_bucket[1m])) * 1000",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "decimals": 3,
          "thresholds": {
            "steps": [
              {"value": 0, "color": "green"},
              {"value": 1, "color": "yellow"},
              {"value": 5, "color": "red"}
            ]
          }
        }
      },
      "options": {
        "colorMode": "background"
      }
    },
    {
      "id": 3,
      "title": "Execution Latency (p99)",
      "type": "stat",
      "gridPos": {"h": 4, "w": 8, "x": 16, "y": 0},
      "targets": [
        {
          "expr": "histogram_quantile(0.99, rate(polymarket_execution_duration_seconds_bucket[1m])) * 1000",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "decimals": 3,
          "thresholds": {
            "steps": [
              {"value": 0, "color": "green"},
              {"value": 100, "color": "yellow"},
              {"value": 500, "color": "red"}
            ]
          }
        }
      },
      "options": {
        "colorMode": "background"
      }
    },
    {
      "id": 4,
      "title": "End-to-End Latency Distribution",
      "type": "heatmap",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 4},
      "targets": [
        {
          "expr": "rate(polymarket_arb_e2e_latency_seconds_bucket[1m])",
          "format": "heatmap",
          "legendFormat": "{{le}}",
          "refId": "A"
        }
      ],
      "options": {
        "calculate": true,
        "cellGap": 2,
        "color": {
          "mode": "scheme",
          "scheme": "RdYlGn",
          "reverse": true
        },
        "yAxis": {
          "axisLabel": "Latency (seconds)",
          "reverse": false
        }
      }
    },
    {
      "id": 5,
      "title": "Orderbook Update Processing Time",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 4},
      "targets": [
        {
          "expr": "histogram_quantile(0.50, rate(polymarket_orderbook_update_processing_duration_seconds_bucket[1m])) * 1000",
          "legendFormat": "p50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, rate(polymarket_orderbook_update_processing_duration_seconds_bucket[1m])) * 1000",
          "legendFormat": "p95",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, rate(polymarket_orderbook_update_processing_duration_seconds_bucket[1m])) * 1000",
          "legendFormat": "p99",
          "refId": "C"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "decimals": 3
        }
      }
    },
    {
      "id": 6,
      "title": "WebSocket Message Latency",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 12},
      "targets": [
        {
          "expr": "histogram_quantile(0.50, rate(polymarket_ws_message_latency_seconds_bucket[1m])) * 1000",
          "legendFormat": "p50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, rate(polymarket_ws_message_latency_seconds_bucket[1m])) * 1000",
          "legendFormat": "p95",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, rate(polymarket_ws_message_latency_seconds_bucket[1m])) * 1000",
          "legendFormat": "p99",
          "refId": "C"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "decimals": 3
        }
      }
    },
    {
      "id": 7,
      "title": "Orderbook Lock Contention",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 12},
      "targets": [
        {
          "expr": "histogram_quantile(0.50, rate(polymarket_orderbook_lock_contention_seconds_bucket[1m])) * 1000000",
          "legendFormat": "p50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, rate(polymarket_orderbook_lock_contention_seconds_bucket[1m])) * 1000000",
          "legendFormat": "p95",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, rate(polymarket_orderbook_lock_contention_seconds_bucket[1m])) * 1000000",
          "legendFormat": "p99",
          "refId": "C"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "µs",
          "decimals": 2
        }
      }
    },
    {
      "id": 8,
      "title": "Cache Operation Latency (by Operation)",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 20},
      "targets": [
        {
          "expr": "histogram_quantile(0.99, rate(polymarket_cache_operation_duration_seconds_bucket{operation=\"get\"}[1m])) * 1000000",
          "legendFormat": "Get (p99)",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.99, rate(polymarket_cache_operation_duration_seconds_bucket{operation=\"set\"}[1m])) * 1000000",
          "legendFormat": "Set (p99)",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, rate(polymarket_cache_operation_duration_seconds_bucket{operation=\"delete\"}[1m])) * 1000000",
          "legendFormat": "Delete (p99)",
          "refId": "C"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "µs",
          "decimals": 2
        }
      }
    },
    {
      "id": 9,
      "title": "Detection Loop Latency Distribution",
      "type": "histogram",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 20},
      "targets": [
        {
          "expr": "polymarket_arb_detection_duration_seconds_bucket",
          "format": "heatmap",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        }
      }
    },
    {
      "id": 10,
      "title": "Latency Summary Table",
      "type": "table",
      "gridPos": {"h": 8, "w": 24, "x": 0, "y": 28},
      "targets": [
        {
          "expr": "histogram_quantile(0.50, rate(polymarket_arb_e2e_latency_seconds_bucket[5m])) * 1000",
          "legendFormat": "E2E p50",
          "refId": "A",
          "format": "table",
          "instant": true
        },
        {
          "expr": "histogram_quantile(0.99, rate(polymarket_arb_e2e_latency_seconds_bucket[5m])) * 1000",
          "legendFormat": "E2E p99",
          "refId": "B",
          "format": "table",
          "instant": true
        },
        {
          "expr": "histogram_quantile(0.50, rate(polymarket_orderbook_update_processing_duration_seconds_bucket[5m])) * 1000",
          "legendFormat": "Orderbook p50",
          "refId": "C",
          "format": "table",
          "instant": true
        },
        {
          "expr": "histogram_quantile(0.99, rate(polymarket_orderbook_update_processing_duration_seconds_bucket[5m])) * 1000",
          "legendFormat": "Orderbook p99",
          "refId": "D",
          "format": "table",
          "instant": true
        },
        {
          "expr": "histogram_quantile(0.50, rate(polymarket_execution_duration_seconds_bucket[5m])) * 1000",
          "legendFormat": "Execution p50",
          "refId": "E",
          "format": "table",
          "instant": true
        },
        {
          "expr": "histogram_quantile(0.99, rate(polymarket_execution_duration_seconds_bucket[5m])) * 1000",
          "legendFormat": "Execution p99",
          "refId": "F",
          "format": "table",
          "instant": true
        }
      ],
      "transformations": [
        {
          "id": "seriesToColumns",
          "options": {}
        }
      ],
      "fieldConfig": {
        "overrides": [
          {
            "matcher": {"id": "byRegexp", "options": ".*"},
            "properties": [
              {"id": "unit", "value": "ms"},
              {"id": "decimals", "value": 3}
            ]
          }
        ]
      }
    }
  ]
}
