{
  "title": "Polymarket System Health",
  "tags": ["polymarket", "health", "operational"],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "5s",
  "templating": {
    "list": [
      {
        "name": "datasource",
        "type": "datasource",
        "query": "prometheus"
      }
    ]
  },
  "panels": [
    {
      "id": 1,
      "title": "WebSocket Connection Status",
      "type": "stat",
      "gridPos": {"h": 4, "w": 6, "x": 0, "y": 0},
      "targets": [
        {
          "expr": "polymarket_ws_active_connections",
          "refId": "A"
        }
      ],
      "options": {
        "colorMode": "background",
        "graphMode": "none"
      },
      "fieldConfig": {
        "defaults": {
          "mappings": [
            {"type": "value", "options": {"0": {"text": "DISCONNECTED", "color": "red"}}},
            {"type": "value", "options": {"1": {"text": "CONNECTED", "color": "green"}}}
          ],
          "thresholds": {
            "steps": [
              {"value": 0, "color": "red"},
              {"value": 1, "color": "green"}
            ]
          }
        }
      }
    },
    {
      "id": 2,
      "title": "Message Processing Latency (p99)",
      "type": "stat",
      "gridPos": {"h": 4, "w": 6, "x": 6, "y": 0},
      "targets": [
        {
          "expr": "histogram_quantile(0.99, rate(polymarket_ws_message_latency_seconds_bucket[1m]))",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "decimals": 6,
          "thresholds": {
            "steps": [
              {"value": 0, "color": "green"},
              {"value": 0.001, "color": "yellow"},
              {"value": 0.01, "color": "red"}
            ]
          }
        }
      }
    },
    {
      "id": 3,
      "title": "Cache Hit Rate",
      "type": "gauge",
      "gridPos": {"h": 4, "w": 6, "x": 12, "y": 0},
      "targets": [
        {
          "expr": "polymarket_cache_hit_rate",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "min": 0,
          "max": 1,
          "thresholds": {
            "steps": [
              {"value": 0, "color": "red"},
              {"value": 0.5, "color": "orange"},
              {"value": 0.8, "color": "green"}
            ]
          }
        }
      }
    },
    {
      "id": 4,
      "title": "Active Subscriptions",
      "type": "stat",
      "gridPos": {"h": 4, "w": 6, "x": 18, "y": 0},
      "targets": [
        {
          "expr": "polymarket_ws_subscription_count",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {"mode": "thresholds"},
          "thresholds": {
            "steps": [
              {"value": 0, "color": "red"},
              {"value": 10, "color": "yellow"},
              {"value": 50, "color": "green"}
            ]
          }
        }
      }
    },
    {
      "id": 5,
      "title": "Execution Errors by Type",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 4},
      "targets": [
        {
          "expr": "rate(polymarket_execution_errors_by_type_total[1m])",
          "legendFormat": "{{error_type}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "custom": {"stacking": {"mode": "normal"}}
        }
      }
    },
    {
      "id": 6,
      "title": "Messages Dropped by Component",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 4},
      "targets": [
        {
          "expr": "rate(polymarket_ws_messages_dropped_total[1m])",
          "legendFormat": "WebSocket",
          "refId": "A"
        },
        {
          "expr": "rate(polymarket_orderbook_updates_dropped_total[1m])",
          "legendFormat": "Orderbook",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "custom": {"stacking": {"mode": "normal"}},
          "color": {"mode": "palette-classic"}
        }
      }
    },
    {
      "id": 7,
      "title": "Orderbook Lock Contention",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 12},
      "targets": [
        {
          "expr": "histogram_quantile(0.50, rate(polymarket_orderbook_lock_contention_seconds_bucket[1m]))",
          "legendFormat": "p50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, rate(polymarket_orderbook_lock_contention_seconds_bucket[1m]))",
          "legendFormat": "p95",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, rate(polymarket_orderbook_lock_contention_seconds_bucket[1m]))",
          "legendFormat": "p99",
          "refId": "C"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "decimals": 6
        }
      }
    },
    {
      "id": 8,
      "title": "WebSocket Reconnection Attempts",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 12},
      "targets": [
        {
          "expr": "rate(polymarket_ws_reconnect_attempts_total[1m])",
          "legendFormat": "Attempts",
          "refId": "A"
        },
        {
          "expr": "rate(polymarket_ws_reconnect_failures_total[1m])",
          "legendFormat": "Failures",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops"
        }
      }
    },
    {
      "id": 9,
      "title": "Cache Operations Latency",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 20},
      "targets": [
        {
          "expr": "histogram_quantile(0.99, rate(polymarket_cache_operation_duration_seconds_bucket{operation=\"get\"}[1m]))",
          "legendFormat": "Get (p99)",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.99, rate(polymarket_cache_operation_duration_seconds_bucket{operation=\"set\"}[1m]))",
          "legendFormat": "Set (p99)",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "decimals": 6
        }
      }
    },
    {
      "id": 10,
      "title": "WebSocket Connection Duration",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 20},
      "targets": [
        {
          "expr": "histogram_quantile(0.50, rate(polymarket_ws_connection_duration_seconds_bucket[5m]))",
          "legendFormat": "Median",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, rate(polymarket_ws_connection_duration_seconds_bucket[5m]))",
          "legendFormat": "p95",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        }
      }
    },
    {
      "id": 11,
      "title": "Discovery Poll Duration",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 28},
      "targets": [
        {
          "expr": "histogram_quantile(0.50, rate(polymarket_discovery_poll_duration_seconds_bucket[1m]))",
          "legendFormat": "p50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.99, rate(polymarket_discovery_poll_duration_seconds_bucket[1m]))",
          "legendFormat": "p99",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        }
      }
    },
    {
      "id": 12,
      "title": "Orderbook Snapshots Tracked",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 28},
      "targets": [
        {
          "expr": "polymarket_orderbook_snapshots_tracked",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      }
    }
  ]
}
